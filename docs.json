{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "iolite",
  "colors": {
    "primary": "#C9A84C",
    "light": "#D4B96A",
    "dark": "#A68A32"
  },
  "background": {
